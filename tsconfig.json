{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "allowJs": true,
    "outDir": "./build",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "ES5",
    "declaration": true
  },
  "include": [
    "./src/**/*"
  ],
}